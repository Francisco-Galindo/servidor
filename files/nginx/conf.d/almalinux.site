alias /srv/almalinux/;

# Directory listing
autoindex on;
autoindex_exact_size off;
autoindex_localtime on;

# Where Nginx will log almalinux requests
access_log /var/log/nginx/almalinux_access.log;
error_log /var/log/nginx/almalinux_error.log;

# Browsers will cache the content for as long as possible
expires max;
