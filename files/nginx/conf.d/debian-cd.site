alias /srv/debian-cd/;

# Directory listing
autoindex on;
autoindex_exact_size off;
autoindex_localtime on;

# Where Nginx will log debian requests
access_log /var/log/nginx/debian-cd_access.log;
error_log /var/log/nginx/debian-cd_error.log;

# Browsers will cache the content for as long as possible
expires max;
